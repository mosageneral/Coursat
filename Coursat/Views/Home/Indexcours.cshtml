@model IEnumerable<Coursat.Models.video>

@{
    ViewBag.Title = "Indexcours";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>محتوى الكورس</h2>


<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Src)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            <iframe width="560" height="315" src="@Html.DisplayFor(modelItem => item.Src)" frameborder="1" allow="autoplay; encrypted-media" allowfullscreen></iframe>
        </td>
        
    </tr>
}

</table>
